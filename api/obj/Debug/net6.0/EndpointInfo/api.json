  "components": {
    "schemas": {
      "CambioMonedaDto": {
        "required": [
          "moneda"
        ],
        "type": "object",
        "properties": {
          "moneda": {
            "minLength": 1,
            "pattern": "^(USD|EUR|GBP)$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CuentaCreacionDto": {
        "required": [
          "division",
          "estado",
          "fechaCreacion",
          "moneda",
          "numeroCuenta",
          "saldo"
        ],
        "type": "object",
        "properties": {
          "numeroCuenta": {
            "type": "integer",
            "format": "int32"
          },
          "division": {
            "minLength": 1,
            "pattern": "^(AHORROS|INVERSIONES|GASTOS)$",
            "type": "string"
          },
          "moneda": {
            "minLength": 1,
            "pattern": "^(USD|EUR|GBP)$",
            "type": "string"
          },
          "saldo": {
            "type": "number",
            "format": "double"
          },
          "fechaCreacion": {
            "type": "string",
            "format": "date-time"
          },
          "estado": {
            "minLength": 1,
            "pattern": "^[NAI]$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CuentaModificacionDto": {
        "type": "object",
        "properties": {
          "numeroCuenta": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "division": {
            "type": "string",
            "nullable": true
          },
          "moneda": {
            "pattern": "^(USD|EUR|GBP)$",
            "type": "string",
            "nullable": true
          },
          "saldo": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "fechaCreacion": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "estado": {
            "pattern": "^[NAI]$",
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SignUpRequest": {
        "required": [
          "password",
          "rol_id",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "rol_id": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization",
        "name": "Authorization",
        "in": "header"
      }
    }
